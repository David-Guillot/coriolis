{
  "name": "svelte-app",
  "version": "1.0.0",
  "devDependencies": {
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "rollup": "^1.26.5",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-livereload": "^1.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-svelte": "^5.0.3",
    "rollup-plugin-terser": "^5.1.2",
    "svelte": "^3.13.0",
    "svelte-preprocess": "^3.1.2"
  },
  "dependencies": {
    "@sveltejs/svelte-virtual-list": "^3.0.1",
    "coriolis": "file:../../",
    "coriolis-dev-tools": "file:../../tools/coriolis-dev-tools/",
    "coriolis-parametered-aggr": "file:../../tools/coriolis-parametered-aggr/",
    "immer": "^5.0.0",
    "sirv-cli": "^0.4.4"
  },
  "scripts": {
    "clear": "rm -rf node_modules public/bundle.css public/*.js public/*.map",
    "build": "rollup -c",
    "autobuild": "rollup -c -w",
    "dev": "run-p start:dev autobuild",
    "dev:remote": "run-p start:dev:remote autobuild",
    "start": "sirv public --single",
    "start:dev": "sirv public --single --dev",
    "start:dev:remote": "npm run -s start:dev -- --host 0.0.0.0"
  }
}

{
  "name": "@coriolis/coriolis",
  "version": "1.0.0-alpha",
  "description": "Event sourced effect management",
  "license": "GPL-3.0",
  "author": "Nicolas Deveaud",
  "repository": "github:coriolisjs/coriolis",
  "bugs": "https://github.com/coriolisjs/coriolis/issues",
  "keywords": [
    "coriolis",
    "event",
    "eventsourcing",
    "effect",
    "aggregate",
    "projection",
    "reducer",
    "predictable",
    "functional",
    "immutable",
    "hot",
    "live",
    "replay",
    "store"
  ],
  "main": "dist/cjs/index.js",
  "type": "module",
  "module": "dist/esm/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "clear:all": "rm -rf node_modules dist",
    "clear": "rm -rf dist",
    "lint": "eslint .",
    "pretty": "prettier './**/{*.{js?(on),md},.*.{js?(on),md}}' --write && eslint . --fix",
    "test": "mocha -r @babel/register -r src/test/init 'src/test/**/*.spec.js'",
    "test:watch": "npm run -s test -- --watch",
    "build": "rollup -c",
    "autobuild": "rollup -c -w",
    "publish:local": "npm run lint && npm run test && npm run clear && npm run build && npm publish --registry http://localhost:4873",
    "publish:npm": "npm run lint && npm run test && npm run clear && npm run build && npm publish --registry https://registry.npmjs.org --access public"
  },
  "devDependencies": {
    "@babel/core": "^7.8.0",
    "@babel/preset-env": "^7.8.0",
    "@babel/register": "^7.8.0",
    "babel-eslint": "^10.0.3",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "dirty-chai": "^2.0.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-chai-expect": "^2.1.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "mocha": "^7.0.1",
    "prettier": "^1.19.1",
    "rollup": "^1.31.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-visualizer": "^3.3.1",
    "sinon": "^8.1.1",
    "sinon-chai": "^3.4.0"
  },
  "dependencies": {
    "rxjs": "^6.5.4"
  }
}

{
  "name": "@coriolis/dev-tools",
  "version": "1.0.0",
  "description": "Dev tools for coriolis",
  "license": "GPL-3.0",
  "author": "Nicolas Deveaud",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "clear": "rm -rf node_modules dist",
    "build": "rollup -c",
    "autobuild": "rollup -c -w",
    "dev": "run-p start:dev autobuild",
    "start": "sirv public --single",
    "start:dev": "sirv public --single --dev",
    "lint": "eslint .",
    "pretty": "prettier './**/{*.{js?(on),ts,y?(a)ml,graphql,md,?(s)css,html},.*.{js?(on),ts,y?(a)ml,graphql,md,?(s)css,html}}' --write && eslint . --fix",
    "publish:local": "npm publish --registry http://localhost:4873"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "node-sass": "^4.13.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "rollup": "^1.29.0",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-livereload": "^1.0.4",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-svelte": "^5.1.1",
    "rollup-plugin-terser": "^5.2.0",
    "svelte": "^3.16.7",
    "svelte-preprocess": "^3.3.0"
  },
  "dependencies": {
    "@sveltejs/svelte-virtual-list": "^3.0.1",
    "@webcomponents/custom-elements": "^1.3.2",
    "@coriolis/coriolis": "^1.0.0",
    "@coriolis/parametered-projection": "^1.0.0",
    "@coriolis/coriolis-svelte": "^1.0.0",
    "immer": "^5.2.1",
    "json-format-highlight": "^1.0.3",
    "sirv-cli": "^0.4.5"
  }
}
